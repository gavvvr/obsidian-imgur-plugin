{
  "name": "obsidian-imgur-plugin",
  "version": "2.5.2",
  "description": "This plugin uploads images from your clipboard to imgur.com and embeds uploaded image to your note",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.js",
    "test": "vitest --coverage",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.js production",
    "prepare": "husky",
    "commit": "git-cz",
    "test:e2e": "wdio run ./test/e2e/wdio.conf.ts"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "keywords": [
    "obsidian.md",
    "imgur.com"
  ],
  "author": "Kirill Gavrilov",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "@wdio/cli": "^8.35.1",
    "@wdio/local-runner": "^8.35.1",
    "@wdio/mocha-framework": "^8.35.0",
    "@wdio/spec-reporter": "^8.32.4",
    "builtin-modules": "^3.3.0",
    "clipboardy": "^4.0.0",
    "commitizen": "^4.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "obsidian": "^1.5.7",
    "obsidian-plugin-cli": "^0.9.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.4.0",
    "vitest": "^1.4.0",
    "wdio-electron-service": "^6.4.0"
  },
  "lint-staged": {
    "*.{ts,js,css,md}": "prettier --write",
    "*.ts": "eslint --cache --fix"
  }
}
